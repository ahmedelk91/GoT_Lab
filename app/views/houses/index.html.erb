<h1 id="title">GAME OF THRONES </h1>
<h3><%= link_to "New House", new_house_path %></h3>

  <% @houses.each do |house| %>
  <ul>
    <li><%= link_to house.name, house_path(house) %></li>
    <p><%= link_to 'Edit', edit_house_path(house) %>
    <%= link_to 'Delete', house_path(house),
    method: :delete,
    data: { confirm: 'Are you sure?'}%></p>

    <!-- i'd remove this line, you have this on line 6 -->
    <!-- <td><%= link_to 'Show', house_path(house) %></td> -->
  </ul>
  <% end %>

